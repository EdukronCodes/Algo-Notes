dim\n","    self.encoder=tf.keras.Sequential([layers.Flatten(),layers.Dense(latent_dim, activation='relu'),])\n","    self.decoder=tf.keras.Sequential([layers.Dense(784, activation='sigmoid'),layers.Reshape((28,28))])\n","\n","  def call(self,x):\n","    encoded=self.encoder(x)\n","    decoded=self.decoder(encoded)\n","    return decoded\n","\n","autoencoder=Autoencoder(latent_dim)"],"metadata":{"id":"dxdHx8i0tJgR"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["## Define Auto Encoder class"],"metadata":{"id":"JaQfz17qe8dn"}},{"cell_type":"code","source":["autoencoder.compile(optimizer='adam', loss=losses.MeanSquaredError())"],"metadata":{"id":"JBJ0KiB8fBr0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["autoencoder.fit(x_train, x_train, epochs=10, shuffle=True, validation_data=(x_val, x_val))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"0De9n9tFfNDh","executionInfo":{"status":"ok","timestamp":1702359016738,"user_tz":-330,"elapsed":92831,"user":{"displayName":"Eisha Akanskha","userId":"01502528335957365622"}},"outputId":"c93d39f8-1480-420b-b863-6a0098dd643c"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch 1/10\n","1563/1563 [==============================] - 9s 5ms/step - loss: 0.0266 - val_loss: 0.0109\n","Epoch 2/10\n","1563/1563 [==============================] - 8s 5ms/step - loss: 0.0080 - val_loss: 0.0062\n","Epoch 3/10\n","1563/1563 [==============================] - 8s 5ms/step - loss: 0.0055 - val_loss: 0.0050\n","Epoch 4/10\n","1563/1563 [==============================] - 7s 5ms/step - loss: 0.0048 - val_loss: 0.0047\n","Epoch 5/10\n","1563/1563 [==============================] - 15s 10ms/step - loss: 0.0045 - val_loss: 0.0044\n","Epoch 6/10\n","1563/1563 [==============================] - 13s 8ms/step - loss: 0.0043 - val_loss: 0.0043\n","Epoch 7/10\n","1563/1563 [==============================] - 8s 5ms/step - loss: 0.0042 - val_loss: 0.0042\n",